{
	"version": 2,
	"builds": [
		{ "src": "api/index.js", "use": "@vercel/node" }
	],
	"routes": [
		{ "src": "/api/(.*)", "dest": "/api/index.js" }
	],
	"env": {
		"MONGODB_URI": "",
		"FRONTEND_URL": ""
	},
	"functions": {
		"api/index.js": {
			"memory": 256,
			"maxDuration": 10,
			"runtime": "nodejs20.x"
		}
	}
}
